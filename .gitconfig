[diff]
  mnemonicprefix = true
  renames = copies
  context = 2
[push]
  default = simple
[status]
  showUntrackedFiles = all
[alias]
  hobnail = push --force
  pluff = pull --ff-only
  meffe = merge --ff-only
  liph = log --graph --oneline --decorate --branches
  brout = checkout -b
  dead = diff HEAD
  died = diff --cached
[instaweb]
  httpd = webrick
  browser = chromium
